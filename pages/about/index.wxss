.index-container {
  margin: 40rpx;
}

.logo-img {
  width: 100%;
  height: 170rpx;
  margin-top: 5%;
}

.app-name {
  width: 100%;
  text-align: center;
  font-size: 38rpx;
  margin-top: 30rpx;
}

.tv-content {
  font-size: 28rpx;
  line-height: 38rpx;
  color: #B3B4B9;
  margin-top: 30rpx;
}

.coop {
  height: 130rpx;
  margin-top: 100rpx;
  border-bottom: 1px solid #EDEDED;
}

.coop .group {
  display: inline-block;
}

.group .tv-title {
  height: 65rpx;
  line-height: 75rpx;
  font-size: 38rpx;
}

.group .tv-email {
  height: 55rpx;
  font-size: 28rpx;
  color: #B3B4B9;
  margin-top: 10;
}

.btn-copy {
  width: 200rpx;
  height: 80rpx;
  line-height: 80rpx;
  float: right;
  color:#fff;
  background: #FFBA39;
  font-size: 28rpx;
  border-radius: 50rpx;
  margin-top: 25rpx;
}